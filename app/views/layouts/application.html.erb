<!DOCTYPE html>
<html>
<head>
  <title>WhatsUp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if notice %>
		<p id="notice"> <%= notice %> </p>
	<% end %>
	<% if alert %>
		<p id="alert"> <%= alert %> </p>
	<% end %>

<%= yield %>

</body>
</html>
